<div class="brands">
  <div class="small-container">
    <div class="row" style="align-items: center;">
      <% for (let category of categories) { %>
        <div class="col-5">
          <a href="/category/<%= category.name %>/<%= category.id %>"><img
              src="/static/image/brands/<%= category.icon %>" alt="<%= category.name %>"></a>
        </div>
        <%}%>
    </div>
  </div>
</div>